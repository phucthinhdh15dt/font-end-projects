{
	"root": true,
	"parser": "babel-eslint",
	"extends": ["@next-zero"],
	"plugins": [],
	"settings": {
		"import/resolver": {
			"alias": { "map": [["App", "./"]], "extensions": [".json", ".js", ".jsx", ".scss", "ts", "tsx"] }
		}
	},
	"rules": {
		"react/jsx-props-no-spreading": 0,
		"react/state-in-constructor": 0,
		"no-param-reassign": "warn",
		"radix": 0
	}
}
